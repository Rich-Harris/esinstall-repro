#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/bin/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/mkdirp@1.0.4/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules:/Users/richard/Development/SNOWPACK/node_modules:/Users/richard/Development/node_modules:/Users/richard/node_modules:/Users/node_modules:/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/node_modules/mkdirp/bin/cmd.js/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/node_modules/mkdirp/bin/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/node_modules/mkdirp/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mkdirp/bin/cmd.js" "$@"
else 
  exec node  "$basedir/../mkdirp/bin/cmd.js" "$@"
fi
