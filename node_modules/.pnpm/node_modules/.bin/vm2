#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/vm2@3.9.2/node_modules/vm2/bin/vm2/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/vm2@3.9.2/node_modules/vm2/bin/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/vm2@3.9.2/node_modules/vm2/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/vm2@3.9.2/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules:/Users/richard/Development/SNOWPACK/node_modules:/Users/richard/Development/node_modules:/Users/richard/node_modules:/Users/node_modules:/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/node_modules/vm2/bin/vm2/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/node_modules/vm2/bin/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/node_modules/vm2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vm2/bin/vm2" "$@"
else 
  exec node  "$basedir/../vm2/bin/vm2" "$@"
fi
