#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/rollup@2.34.1/node_modules/rollup/dist/bin/rollup/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/rollup@2.34.1/node_modules/rollup/dist/bin/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/rollup@2.34.1/node_modules/rollup/dist/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/rollup@2.34.1/node_modules/rollup/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/rollup@2.34.1/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules:/Users/richard/Development/SNOWPACK/node_modules:/Users/richard/Development/node_modules:/Users/richard/node_modules:/Users/node_modules:/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/@rollup/plugin-replace@2.3.4_rollup@2.34.1/node_modules/@rollup/plugin-replace/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/@rollup/plugin-replace@2.3.4_rollup@2.34.1/node_modules/@rollup/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/@rollup/plugin-replace@2.3.4_rollup@2.34.1/node_modules:/Users/richard/Development/SNOWPACK/esinstall-repro/node_modules/.pnpm/@rollup/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../rollup@2.34.1/node_modules/rollup/dist/bin/rollup" "$@"
else 
  exec node  "$basedir/../../../../../../../rollup@2.34.1/node_modules/rollup/dist/bin/rollup" "$@"
fi
